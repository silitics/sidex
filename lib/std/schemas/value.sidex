//! Extension for representing values of unknown type.

variant Value {
    String: string,
    Bytes: bytes,

    I8: i8,
    I16: i16,
    I32: i32,
    I64: i64,

    U8: u8,
    U16: u16,
    U32: u32,
    U64: u64,

    Idx: idx,

    F32: f32,
    F64: f64,

    Bool: bool,

    Unit,

    Sequence: [Value],
    Map: [Value: Value],    
}
